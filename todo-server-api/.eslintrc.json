{
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended", "google", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "indent": ["error", 2],
    "prettier/prettier": "error",
    "quote-props": 1
  }
}
